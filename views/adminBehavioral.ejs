<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Behavioral Dashboard</title>
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    <h1>Behavioral Questions Dashboard</h1>
    <form action="/admin/behavioral/createBehavioralQuestion" method="post">
      <input
        type="text"
        placeholder="Add a new behavioral question"
        name="addBehavioralQuestion"
      />
      <button type="submit">Submit</button>
    </form>
    <ul>
      <% behaviorals.forEach( el => { %>
              <li class='todoItem' data-id='<%=el._id%>'>
                <span class="edit">Edit</span>
                  <span class='del'> Delete </span>
                  <span class='<%= el.completed === true ? 'completed' : 'not'%>'><%= el.behavioralQuestion %></span>
                  
              </li>
      <% }) %>    
      </ul>
      <script src="/js/app.js"></script>
  </body>
</html>
